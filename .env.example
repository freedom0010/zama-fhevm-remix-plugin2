# Private Keys (DO NOT COMMIT TO VERSION CONTROL)
PRIVATE_KEY=your_private_key_here
DEPLOYER_ADDRESS=your_deployer_address_here

# RPC Endpoints
SEPOLIA_RPC_URL=https://sepolia.infura.io/v3/your_infura_key
FHEVM_SEPOLIA_RPC_URL=https://devnet.zama.ai
FHEVM_MAINNET_RPC_URL=https://mainnet.zama.ai

# API Keys for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key
FHEVM_EXPLORER_API_KEY=your_fhevm_explorer_api_key

# Gas settings
GAS_PRICE_GWEI=20
GAS_LIMIT=5000000
REPORT_GAS=true

# Deployment configuration
NETWORK=fhevm-sepolia
VERIFY_CONTRACTS=true
SAVE_DEPLOYMENT=true
DEPLOYMENT_FILE=./deployment/deployments.json

# FHE Token Configuration
TOKEN_NAME=ZamaToken
TOKEN_SYMBOL=ZAMA
TOKEN_INITIAL_SUPPLY=1000000

# FHE NFT Configuration
NFT_NAME=ZamaNFT
NFT_SYMBOL=ZNFT
NFT_BASE_URI=https://ipfs.io/ipfs/